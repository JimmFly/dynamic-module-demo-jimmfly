{
  "name": "dynamic-module-executor",
  "version": "1.0.0",
  "description": "A high-performance dynamic module executor with hot reloading capabilities",
  "main": "dist/src/main.js",
  "scripts": {
    "start": "npm run build && node dist/src/main.js",
    "dev": "ts-node src/main.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prestart": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "dependencies": {
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "dynamic",
    "module",
    "hot-reload",
    "sqlite",
    "typescript",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
